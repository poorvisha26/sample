import java.io.*;
import java.util.*;
public class palindrome
{
public static void main(String s[])
{
int n,pal=0,temp,s;
Scanner sc=new Scanner(System.in);
n=sc.nextInt();
temp=n;
while(n>0)
{
s=n%10;
pal=(pal*10)+s;
n/=10;
}
if(temp==pal)
printf(pal+"it is a palindrome");
else
printf(pal+"it is not a palindrome");
}
}
